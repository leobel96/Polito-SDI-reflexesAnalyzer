--synthesis_resources = lut 8 
SUBDESIGN cmpr_m7j
( 
	agb	:	output;
	dataa[7..0]	:	input;
	datab[7..0]	:	input;
) 
VARIABLE
	agb_int	:	WIRE;
BEGIN 
	IF (dataa[] > datab[]) THEN
		agb_int = VCC;
	ELSE
		agb_int = GND;
	END IF;
	agb = agb_int;
END;
